{
  "appId": "<%= @application_id -%>",
  "masterKey": "<%= @master_key -%>",
  "databaseURI": "<%= @database_uri -%>",
  "port": "<%= @port -%>",
<% if @cloud_code -%>
  "cloud": "<%= @parse_root -%>/<%= @app_name -%>/cloud/main.js",
<% end -%>
<% if @file_key -%>
  "fileKey": "<%= @file_key -%>",
<% end -%>
<% if @javascript_key -%>
  "javascriptKey": "<%= @javascript_key -%>",
<% end -%>
<% if @rest_key -%>
  "restAPIKey": "<%= @rest_key -%>",
<% end -%>
<% if @client_key -%>
  "clientKey": "<%= @client_key -%>",
<% end -%>
<% if @public_url -%>
  "publicServerURL": "<%= @public_url -%>",
<% end -%>
<% if @aws_s3 -%>
  "filesAdapter": {
    "module": "parse-server-s3-adapter",
    "options": {
      "accessKey": "<%= @aws_s3_access_key -%>",
      "secretKey": "<%= @aws_s3_secret_key -%>",
      "bucket": "<%= @aws_s3_bucket -%>",
      "region": "<%= @aws_s3_region -%>",
      "bucketPrefix": "<%= @aws_s3_bucket_prefix -%>",
      "directAccess": <%= @aws_s3_direct_access -%>,
      "baseUrl": "<%= @aws_s3_base_url -%>",
      "baseUrlDirect": <%= @aws_s3_base_url_direct %>
    }
  },
<% end -%>
<% if @mailgun -%>
  "verifyUserEmails": true,
  "emailAdapter": {
    "module": "parse-server-simple-mailgun-adapter",
    "options": {
      "fromAddress": "<%= @mailgun_from_address -%>",
      "domain": "<%= @mailgun_domain -%>",
      "apiKey": "<%= @mailgun_api_key -%>"
    }
  },
<% end -%>
  "appName": "<%= @app_name -%>"
}
